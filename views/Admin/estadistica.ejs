      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
       
          
</head>

    <body>


<!-- Barra de navegaciÃ³n -->
<body>
  

<header>
   <nav>
   
  <nav>
    <div class="nav-wrapper transparent">

    <a href="/" class="brand-logo"><i class="material-icons">home</i></a>
    <a href="#" data-activates="mobile-demo" class="button-collapse"><i class="material-icons">menu</i></a>

    <ul class="right hide-on-med-and-down">
      <li><a href="/Admin/perfil"><i class="material-icons">account_circle</i></a></li>
         
    </div>
  </nav>
</nav>
</header>


<section>
<br>
<br>
<br>
<div class="container">
<div class="row s12">
  
  <div class="col m6">
    
    <h4>Vehiculos mas atendidos</h4>
<br>
<br>

<table align="center">
<tr> 
  <th class="lato text-center"> Modelo </th>
  <th class="lato text-center"> Cantidad de veces atendido </th>
</tr>


<%_.each (Nombre, function(top){ %>

<tr data-id= "<%= Nombre.id %>" data-model="Mantenimiento" >
<td class="lato "><%= top.modelo %></td>
<td class="lato "><%= top.numero %></td>
</tr>

<% }) %>
</table>

  </div>


  <div class="col m6">

      <h4>Servicios mas solicitados</h4>
<br>
<br>

<table align="center">
<tr> 
  <th class="lato text-center"> Servicio </th>
  <th class="lato text-center"> Cantidad de veces solicitado </th>
</tr>


<%_.each (Servicio, function(top){ %>

<tr data-id= "<%= Servicio.id %>" data-model="Servicio" >
<td class="lato "><%= top.nombre %></td>
<td class="lato "><%= top.numero %></td>
</tr>

<% }) %>
</table>
    


  </div>


</div>


</div>

    </div>

  </div>



  </div>
</section>

